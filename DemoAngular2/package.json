        "@angular/platform-server": "^4.0.0",
        "@angular/router": "^4.0.0",
        "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.22",
        "@types/node": "^6.0.66",
        "@types/selenium-webdriver": "^2.53.42",
        "angular-in-memory-web-api": "^0.2.5",
        "concurrently": "^3.4.0",
        "core-js": "^2.4.1",
        "karma": "^1.5.0",
        "moment": "^2.18.1",
        "reflect-metadata": "^0.1.10",
        "rimraf": "^2.6.1",
        "rxjs": "^5.2.0",
        "systemjs": "^0.19.47",
        "typescript": "^2.2.2",
